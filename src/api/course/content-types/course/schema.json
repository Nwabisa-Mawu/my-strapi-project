{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "coursename": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50,
      "minLength": 5
    },
    "course_description": {
      "type": "text",
      "required": true,
      "maxLength": 120,
      "minLength": 10
    },
    "course_price": {
      "type": "string",
      "default": "Price Unavailable"
    },
    "course_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "users_code": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "course"
    }
  }
}
